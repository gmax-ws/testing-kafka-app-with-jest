{
    "env": {
        "browser": false,
        "es6": true
    },
    "globals": {
        "consumerAppInboundMessages": "writeable",
        "producerAppOutboundMessages": "writeable",
        "fetch": "readable"
    },
    "extends": "eslint-config-airbnb-base/legacy",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        },
        "sourceType": "module"
    },
    "overrides": [
        {
            "files": [
                "**/*.test.js"
            ],
            "env": {
                "jest": true
            }
        }
    ],
    "plugins": [
        "jest",
        "jsdoc",
        "import"
    ],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "jest/no-disabled-tests": "warn",
        "jest/no-focused-tests": "error",
        "jest/no-identical-title": "error",
        "jest/prefer-to-have-length": "warn",
        "jest/valid-expect": "error",
        "no-console": "off",
        "consistent-return": "off"
    }
}
